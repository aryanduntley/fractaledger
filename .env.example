# Litecoin Wallet Secrets
LTC_WALLET_1_SECRET=your_ltc_wallet_1_private_key_here
LTC_WALLET_2_SECRET=your_ltc_wallet_2_private_key_here
LTC_WALLET_3_SECRET=your_ltc_wallet_3_private_key_here

# Dogecoin Wallet Secrets
DOGE_WALLET_1_SECRET=your_doge_wallet_1_private_key_here
DOGE_WALLET_2_SECRET=your_doge_wallet_2_private_key_here
DOGE_WALLET_3_SECRET=your_doge_wallet_3_private_key_here

# API Keys for Blockchain API Services
BLOCKCYPHER_API_KEY=your_blockcypher_api_key_here
BLOCKCHAIR_API_KEY=your_blockchair_api_key_here

# Full Node RPC Credentials
BITCOIN_RPC_USERNAME=bitcoinrpc
BITCOIN_RPC_PASSWORD=your_bitcoin_rpc_password_here
LITECOIN_RPC_USERNAME=litecoinrpc
LITECOIN_RPC_PASSWORD=your_litecoin_rpc_password_here
DOGECOIN_RPC_USERNAME=dogecoinrpc
DOGECOIN_RPC_PASSWORD=your_dogecoin_rpc_password_here

# JWT Secret for API Authentication
JWT_SECRET=your_jwt_secret_here_min_32_chars

# Hyperledger Fabric Credentials
# These are typically stored in wallet files, but can be referenced here if needed
HYPERLEDGER_ADMIN_CERT=path_to_admin_certificate
HYPERLEDGER_ADMIN_KEY=path_to_admin_private_key

# Database Encryption Key (if applicable)
DB_ENCRYPTION_KEY=your_encryption_key_here

